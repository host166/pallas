.range {
    position relative
    height 1.34rem
}

.range-value span {
    display inline-block
}

.range-wrap {
    position absolute
    right .61rem
    left .61rem
}

.range-left,
.range-right {
    position absolute
    width 1.22rem
    height 1.34rem
    background-image url('data:image/png;base64,')
    background-size contain
    background-repeat no-repeat
    background-position center center
    z-index 1
}

.range-left {
    left 0
}

.range-right {
    right 0
}

.range-progress {
    height .08rem
    width 100%
    display block
    background-color #F24
    position absolute
    top .63rem
}

.range-progress-left,
.range-progress-right {
    position absolute
    background-color #E1E1E1
    height .08rem
    display block
}

.range-progress-left {
    left 0
}

.range-progress-right {
    right 0
}

.range-desc {
    display -webkit-box
    position absolute
    bottom 0
    width 100%
    display none
}

.range-desc li {
    -webkit-box-flex 1
    text-align center
    width 1px
}

.range-desc li span {
    margin-left -100%
}
